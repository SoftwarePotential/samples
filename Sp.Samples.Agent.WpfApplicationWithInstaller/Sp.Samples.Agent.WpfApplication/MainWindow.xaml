<Window x:Class="Sp.Samples.Agent.WpfApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="Sp.Samples.Agent.WpfApplication" Height="350" Width="525">
	<Grid>
		<DockPanel LastChildFill="True">
			<Menu IsMainMenu="True" DockPanel.Dock="Top">
				<MenuItem Header="_Licenses"   >
					<MenuItem Header="List licenses" Click="ShowInstalledLicensesList_Click"></MenuItem>
					<MenuItem Header="Activate" Click="ShowActivationDialog_Click"></MenuItem>
				</MenuItem>
			</Menu>
			<StackPanel Margin="10 10">
				<Button Name="runFeatureA" HorizontalAlignment="Left"  VerticalAlignment="Top" Margin="0 10" Height="30" Click="RunFeatureA_Click" >Run Feature A</Button>
				<Button Name="runFeatureB" HorizontalAlignment="Left"  VerticalAlignment="Top" Margin="0 10" Height="30" Click="RunFeatureB_Click">Run Feature B</Button>
			</StackPanel>
		</DockPanel>
	</Grid>
</Window>
